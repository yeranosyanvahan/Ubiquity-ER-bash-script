
#TEST IF EXISTS

ls /opt/vyatta/config/active/firewall/group/address-group > ./tmp/tmp.txt
if test ! "$(cat ./tmp/tmp.txt | grep $TARIFF | head -1)" = "$TARIFF";then
echo -e "\nthe tariff $TARIFF does not exist\n"
exit 1
fi


$begin
$set firewall group address-group $TARIFF address $IP
$commit
$discard
$end
